#!/usr/bin/env groovy
properties([[$class: 'GithubProjectProperty', displayName: '', projectUrlStr: 'https://github.com/sindhu88/Task7.git/'], pipelineTriggers([])])

pipeline {
   agent any 

   stages {
       stage('Build') {
           steps {
               sh 'pwd'
           }
       }
       stage('Test'){
           steps {
               sh 'java -version'
               
           }
       }
       stage('Deploy') {
           steps {
               sh 'ls'
               sh 'pwd'
           }
       }
   }
}
